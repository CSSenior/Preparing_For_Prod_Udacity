e unused and unnecessary styling and selectors */\r\n/* Card Set Styling */\r\nbody main div .cardSets {\r\n  width: 85%;\r\n  height: 100%;\r\n  margin: 5%;\r\n  padding: 10px;\r\n  border: 2px solid lightgray;\r\n  border-radius: 10px;\r\n  box-shadow: 0 2px 5px lightgray;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\nbody main div .cardSets:hover {\r\n  border-color: black;\r\n}\r\n\r\n/* Flip Card Styling */\r\nbody main div .cardContainer {\r\n  background-color: transparent;\r\n  width: 500px;\r\n  height: 380px;\r\n  margin: 5px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\nbody main div.innerCard {\r\n  position: relative;\r\n  width: 100%;\r\n  height: 100%;\r\n  text-align: center;\r\n  transition: transform 1.5s;\r\n  transform-style: preserve-3d;\r\n}\r\n\r\n.cardContainer:hover .innerCard {\r\n  transform: rotateY(180deg);\r\n}\r\n\r\n/* Term and Description Styling */\r\n.term,\r\n.description {\r\n  position: absolute;\r\n  width: 100%;\r\n  height: 100%;\r\n  backface-visibility: hidden;\r\n  border-radius: 10%;\r\n  border: solid grey;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  color: black;\r\n}\r\n\r\n.term {\r\n  font-size: 2rem;\r\n}\r\n\r\nbody main div .description {\r\n  font-size: 1.5rem;\r\n  background-color: #dce7e8;\r\n  transform: rotateY(180deg);\r\n}\r\n\r\n/* Page and Container Layout */\r\nbody main div .cardPageContainer {\r\n  width: 500px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-around;\r\n}\r\n\r\n.cardPageContainer button {\r\n  margin: 3%;\r\n}\r\n\r\n.cardPageContainer h2 {\r\n  text-align: center;\r\n}\r\n\r\n.setContainer {\r\n  margin: 0;\r\n  padding: 0;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n}\r\n\r\n/* Navigation Buttons Styling */\r\n.nextAndPrevBtnContainer {\r\n  display: flex;\r\n  justify-content: space-around;\r\n  margin: 5%;\r\n}\r\n\r\n.nextAndPrevBtnContainer button {\r\n  width: 45%;\r\n}\r\n\r\n/* Miscellaneous */\r\n.toggle_form {\r\n  text-align: center;\r\n}\r\n\r\n/* Media Query for Tablets (768px and below) */\r\n@media (max-width: 768px) {\r\n  .cardContainer {\r\n    width: 90%;\r\n    height: 200px;\r\n  }\r\n\r\n  .term {\r\n    font-size: 1.8rem;\r\n  }\r\n\r\n  .description {\r\n    font-size: 1.3rem;\r\n  }\r\n\r\n  .cardPageContainer {\r\n    width: 90%;\r\n  }\r\n\r\n  .nextAndPrevBtnContainer {\r\n    flex-direction: column;\r\n    align-items: center;\r\n  }\r\n\r\n  .nextAndPrevBtnContainer button {\r\n    width: 80%;\r\n    margin: 5px 0;\r\n  }\r\n}\r\n\r\n/* Media Query for Mobile Devices (480px and below) */\r\n@media (max-width: 480px) {\r\n  .cardContainer {\r\n    width: 100%;\r\n    height: 200px;\r\n  }\r\n\r\n  .term {\r\n    font-size: 1.5rem;\r\n  }\r\n\r\n  .description {\r\n    font-size: 1.2rem;\r\n  }\r\n\r\n  .cardPageContainer {\r\n    width: 100%;\r\n  }\r\n\r\n  .nextAndPrevBtnContainer {\r\n    flex-direction: column;\r\n    align-items: center;\r\n    margin: 5%;\r\n  }\r\n\r\n  .nextAndPrevBtnContainer button {\r\n    width: 90%;\r\n    margin: 5px 0;\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"9deec82b8efb27329843342c00c0f318","cacheData":{"env":{}}}