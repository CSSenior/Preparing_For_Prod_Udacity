nt(\"input\");\r\n  submit.type = \"submit\";\r\n  submit.value = value;\r\n\r\n  return submit;\r\n};\r\n\r\nexport {\r\n  createElement,\r\n  createImage,\r\n  createHeader,\r\n  createToggleButton,\r\n  createLabel,\r\n  createInput,\r\n  createSubmitButton,\r\n};\r\n"},"lineCount":null}},"error":null,"hash":"b74328c7e91eb5fd4430bb65767e6408","cacheData":{"env":{}}}